package dao;

public class JdbcUtil {
	private static JdbcUtil instance = new JdbcUtil();
	
	static {
		try{
			Class.forName("com.mysql.cj.jdbc.Driver");
			System.out.println("드라이버 로딩 성공!");
		}catch(ClassNotFoundException e){
			e.printStackTrace();
		}
	}
	
	private JdbcUtil(){}
	
	public JdbcUtil getInstance(){
		return instance;
	}
}
